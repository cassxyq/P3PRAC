{
    "family": "ecsprac-td",
    "networkMode": "awsvpc", 
    "containerDefinitions": [
        {
            "image": "327746137438.dkr.ecr.ap-southeast-2.amazonaws.com/ecsprac:latest",
            "name": "pracapp",
            "essential": true,
            "portMappings": [
                {
                    "containerPort": 3000,
                    "hostPort": 3000
                }
            ]
        }
    ],
    "cpu": "256",
    "memory": "512",
    "executionRoleArn": "arn:aws:iam::327746137438:role/ecsTaskExecutionRole"
